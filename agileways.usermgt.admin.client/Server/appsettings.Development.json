{
  "AzureAdB2C": {
    "Instance": "https://<your-tenant-name>.b2clogin.com/",
    "ClientId": "<server-app-registration-client-id>",
    "Domain": "<your-tenant-name>.onmicrosoft.com",
    "SignUpSignInPolicyId": "<your-SUSI-policy-name>"
  },
  "AzureAdB2CGraph": {
    "Instance": "https://login.microsoftonline.com/{0}",
    "ApiUrl": "https://graph.microsoft.com/",
    "Tenant": "<your-tenant's-GUID>",
    "ClientId": "<graph-app-registration-client-id>",
    "ClientSecret": "<graph-app-registration-secret-value>"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}